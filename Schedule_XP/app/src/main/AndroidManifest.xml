<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yukinaito.schedule_xp">

    <application
        android:name=".SchedlueApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingModelActivity"></activity>
        <activity android:name=".AddModelActivity"></activity>
        <activity android:name=".CheckPlanActivity"></activity>
        <activity android:name=".AddPlanActivity"></activity>
        <activity android:name=".AddHavetoPlanActivity"></activity>
    </application>

</manifest>